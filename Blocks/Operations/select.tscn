[sub_resource type="GDScript" id="GDScript_2bore"]
resource_name = "select"
script/source = "extends LogicBlocks

func load_value(value: Array):
	%a.text = value[1]
	option_get_item(%op1,value[2])
	%b.text = value[3]
	%c.text = value[4]
	%d.text = value[5]
	%e.text = value[6]
	
	
	pass

func option_get_item(node: OptionButton, optext : String):
	for i in range(node.get_popup().item_count):
		if op_list[i] == optext:
			node.select(i)
			_on_op_1_item_selected(i)


var op_list = {
		0 : \"equal\" ,
		1 : \"notEqual\" ,
		2 : \"lessThan\" ,
		3 : \"lessThanEq\" ,
		4 : \"greaterThan\" ,
		5 : \"greaterThanEq\" ,
		6 : \"strictEqual\" ,
		7 : \"always\" ,
	}

func compile():
	
	return(\"select \" + %a.text + \" \" + op_list[%op1.selected] + \" \" + %b.text + \" \" + %c.text + \" \" + %d.text + \" \" + %e.text +\"\\n\" )


func _on_op_1_item_selected(index: int) -> void:
	if index == 7:
		%b.visible = false
	else:
		%b.visible = true
	pass # Replace with function body.
"

