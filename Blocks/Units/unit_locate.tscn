[sub_resource type="GDScript" id="GDScript_ggotj"]
resource_name = "unit_locate"
script/source = "extends LogicBlocks

func load_value(value: Array):
	option_get_item(%op2,value[2])
	option_get_item(%op1,value[1])
	%a.text = value[3]
	%b.text = value[4]
	%c.text = value[5]
	%d.text = value[6]
	%e.text = value[7]
	%f.text = value[8]
	
	pass

func option_get_item(node: OptionButton, optext : String):
	for i in range(node.get_popup().item_count):
		if node.get_item_text(i) == optext:
			node.select(i)
			_on_op_1_item_selected(i)


func compile():
	
	var op_list1 = {
		0 : \"ore\" ,
		1 : \"building\" ,
		2 : \"spawn\" ,
		3 : \"damaged\" ,
	}
	
	var op_list2 = {
		0 : \"core\" ,
		1 : \"stroage\" ,
		2 : \"generator\" ,
		3 : \"turret\" ,
		4 : \"factory\" ,
		5 : \"repair\" ,
		6 : \"battery\" ,
		7 : \"reactor\" ,
		8 : \"drill\" ,
		9 : \"shield\" ,
	}
	
	return(\"ulocate \" + op_list1[%op1.selected] + \" \" + op_list2[%op2.selected] + \" \" + %a.text + \" \" + %b.text + \" \" + %c.text + \" \" + %d.text + \" \" + %e.text + \" \" + %f.text + \"\\n\")
	
	pass




func _on_op_1_item_selected(index: int) -> void:
	
	if index == 0:
		
		%Label1.visible = false
		%Label2.visible = false
		%Label4.visible = false
		%op2.visible = false
		%f.visible = false
		%a.visible = false
		
		%Label3.visible = true
		%b.visible = true
	
	elif index == 1:
		
		%Label1.visible = true
		%Label2.visible = true
		%Label4.visible = true
		%op2.visible = true
		%a.visible = true
		%f.visible = true
		
		%Label3.visible = false
		%b.visible = false
		
	elif index == 2 or index == 3:
		
		%Label1.visible = false
		%Label2.visible = false
		%Label3.visible = false
		%Label4.visible = false
		%op2.visible = false
		%a.visible = false
		%b.visible = false
		%f.visible = false
		
	
	pass # Replace with function body.
"

