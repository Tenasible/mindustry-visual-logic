[sub_resource type="GDScript" id="GDScript_37m3r"]
resource_name = "control"
script/source = "extends LogicBlocks

func load_value(value: Array):
	option_get_item(%op1,value[1])
	%a.text = value[2]
	%b.text = value[3]
	%c.text = value[4]
	%d.text = value[5]
	%e.text = value[6]
	
	
	pass

func option_get_item(node: OptionButton, optext : String):
	for i in range(node.get_popup().item_count):
		if node.get_item_text(i) == optext:
			node.select(i)
			_on_op_1_item_selected(i)
			
	

func compile():
	
	var op_list = {
		0 : \"enabled\" ,
		1 : \"shoot\" ,
		2 : \"shootp\" ,
		3 : \"config\" ,
		4 : \"color\" ,
	}
	
	return(\"control \" + op_list[%op1.selected] + \" \" + %a.text + \" \" + %b.text + \" \" + %c.text + \" \" + %d.text + \" \" + %e.text + \"\\n\")




func _on_op_1_item_selected(index: int) -> void:
	
	if index == 0 or index == 3 or index == 4:
		
		%Label3.text = \"to\"
		
		%Label4.visible = false
		%Label5.visible = false
		%c.visible = false
		%d.visible = false
		
	elif index == 1:
		
		%Label3.text = \"x\"
		%Label4.text = \"y\"
		
		%Label4.visible = true
		%Label5.visible = true
		%c.visible = true
		%d.visible = true
		
	elif index == 2:
		
		%Label3.text = \"unit\"
		%Label4.text = \"shoot\"
		
		%Label4.visible = true
		%c.visible = true
		
		%Label5.visible = false
		%d.visible = false
	
	pass # Replace with function body.
"

