[sub_resource type="GDScript" id="GDScript_gtlx6"]
resource_name = "radar"
script/source = "extends LogicBlocks

func load_value(value: Array):
	option_get_item(%op1,value[1])
	option_get_item(%op2,value[2])
	option_get_item(%op3,value[3])
	option_get_item(%op4,value[4])
	%a.text = value[5]
	%b.text = value[6]
	%c.text = value[7]
	pass

func option_get_item(node: OptionButton, optext : String):
	for i in range(node.get_popup().item_count):
		if node.get_item_text(i) == optext:
			node.select(i)

func compile():
	var op_list1 = {
		0 : \"any\" ,
		1 : \"enemy\" ,
		2 : \"ally\" ,
		3 : \"player\" ,
		4 : \"attacker\" ,
		5 : \"flying\" ,
		6 : \"boss\" ,
		7 : \"ground\" ,
	}
	
	var op_list2 = {
		0 : \"distance\" ,
		1 : \"health\" ,
		2 : \"shield\" ,
		3 : \"armor\" ,
		4 : \"maxHealth\" ,
	}
	
	return(\"radar \" + op_list1[%op1.selected] + \" \" + op_list1[%op2.selected] + \" \" + op_list1[%op3.selected] + \" \" + op_list2[%op4.selected] + \" \" + %a.text + \" \" + %b.text + \" \" + %c.text + \"\\n\" )

"

